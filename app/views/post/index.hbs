<h1> Post </h1>

<a href="/post/new"><button type="button" class="btn btn-primary btn-block" style="width: 100px"> New Post </button></a>


<script>
function GetPost() {
  success: function (posts) {
      $.each(posts, function (i, post) {
          var a;
          a = `<div class="col-sm-4 friend-block"><label >  ${post.title}  </label><button type="submit" class="btn btn-success" style="margin-left: 20px "">View</button> </div>`
          $postList.append(a);
      })
  }
}
</script>
